<http://data.semanticweb.org/conference/iswc/2008/paper/poster_demo/77> a swrc:InProceedings ;
    rdfs:label "Language as a Foundation of the Semantic Web" ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc/2008/poster_demo_proceedings> ;
