<http://data.semanticweb.org/conference/iswc/2011/paper/research/110> a swrc:InProceedings ;
    rdfs:label "Inspecting regularities in ontology design using clustering" ;
    swc:hasTopic <http://dbpedia.org/resource/Semantic_Web> ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc/2011/proceedings> ;
    swc:relatedToEvent <http://data.semanticweb.org/conference/iswc/2011/talks/research/110> ;
