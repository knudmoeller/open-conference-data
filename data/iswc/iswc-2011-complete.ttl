    rdfs:label "Scalable Integration and Processing of Linked Data" ;
    swc:relatedToEvent <http://data.semanticweb.org/conference/iswc/2011/sessions/poster-session>,
        <http://data.semanticweb.org/conference/iswc/2011/tutorials/5> ;
