    swc:relatedToEvent <http://data.semanticweb.org/conference/iswc/2011/sessions/poster-session> ;
    dc:creator <http://data.semanticweb.org/person/abelardo-pardo>,
        <http://data.semanticweb.org/person/carlos-delgado-kloos>,
