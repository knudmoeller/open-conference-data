<http://data.semanticweb.org/conference/iswc/2011/sessions/conference-dinner> a swc:SocialEvent ;
    rdfs:label "Conference Dinner" ;
    swc:hasLocation <http://data.semanticweb.org/conference/iswc/2011/place/maritim> ;
